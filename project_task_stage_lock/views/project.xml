<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="task_type_tree_inherited" model="ir.ui.view">
        <field
            name="name"
        >project.task.type.list.inherited - project_task_stage_lock</field>
        <field name="model">project.task.type</field>
        <field name="inherit_id" ref="project.task_type_tree_inherited" />
        <field name="arch" type="xml">
            <xpath expr="//list" position="attributes">
                <attribute name="default_group_by" />
            </xpath>
        </field>
    </record>
</odoo>
